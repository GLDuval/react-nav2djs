{
  "compilerOptions": {
    "target": "es2021",
    "allowSyntheticDefaultImports": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "noEmit": true,
    "jsx": "react",
    "baseUrl": ".",
    "skipLibCheck": true,
    "declaration": true,
    "outDir": "dist",
    "paths": {
      "@/*": [
        "./src/*"
      ]
    }
  },
  "include": [
    "src",
  ],
  "exclude": [
    "node_modules",
    "dist"
  ]
}
